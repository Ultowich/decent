{
  "name": "@decent/client",
  "version": "0.0.0",
  "description": "Standalone web client for Decent",
  "main": "index.js",
  "repository": "https://github.com/decent-chat/decent/tree/master/packages/client",
  "keywords": [
    "chat",
    "chat-system",
    "social",
    "decent"
  ],
  "author": "Multiple contributors",
  "license": "GPL-3.0",
  "dependencies": {
    "choo": "^6.6.1",
    "choo-devtools": "^2.3.3",
    "mrk.js": "^1.0.1",
    "nanobus": "^4.3.2",
    "prismjs": "^1.9.0"
  },
  "devDependencies": {
    "browserify": "^15.1.0",
    "css-extract": "^1.2.0",
    "sheetify": "^6.2.0",
    "sheetify-nested": "^1.0.2",
    "watchify": "^3.9.0",
    "uglify-es": "^3.3.9",
    "yo-yoify": "^4.3.0"
  },
  "scripts": {
    "build": "browserify src/app.js -g yo-yoify -t [ sheetify -u sheetify-nested ] -p [ css-extract -o dist/bundle.css ] | uglifyjs --compress --mangle -o dist/bundle.js",
    "watch": "watchify src/app.js -o dist/bundle.js -d -t [ sheetify -u sheetify-nested ] -p [ css-extract -o dist/bundle.css ] -v",
    "prepublishOnly": "npm run build"
  }
}
